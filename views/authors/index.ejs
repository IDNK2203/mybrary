<h2>Search Authors</h2>
<form action="/authors" method="GET">
  <p>
    <label for="name"> Name </label>
    <input type="text" id="name" name="name" value="<%= search_opts %>" />
    <button type="submit">Search</button>
  </p>
</form>

<% authors.forEach((author)=>{ %>
<div><%= author.name %></div>
<% }) %>
